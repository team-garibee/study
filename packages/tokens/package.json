{
  "name": "@mumukji/tokens",
  "version": "0.0.0",
  "description": "Design tokens for Mumukji design system. Includes colors, typography, spacing, and other foundational styles.",
  "private": false,
  "author": "team-garibee",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/team-garibee/mumukji-ui.git",
    "directory": "packages/tokens"
  },
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "watch": "rollup -cw",
    "typecheck": "tsc --noEmit",
    "build": "tsc && rollup -c",
    "clean": "rm -rf dist"
  },
  "keywords": [
    "design-system",
    "design-tokens",
    "tokens",
    "mumukji"
  ],
  "devDependencies": {
    "rollup": "^4.58.0"
  }
}
