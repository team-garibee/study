{
  "name": "@mumukji/tokens",
  "version": "0.0.0",
  "description": "Design tokens for Mumukji design system. Includes colors, typography, spacing, and other foundational styles.",
  "private": false,
  "author": "team-garibee",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/team-garibee/mumukji-ui.git",
    "directory": "packages/tokens"
  },
  "main": "dist/index.cjs.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.cjs.js",
      "import": "./dist/index.esm.js",
      "types": "./dist/index.d.ts"
    }
  },
  "sideEffects": false,
  "scripts": {
    "typecheck": "tsc --noEmit",
    "build": "rollup -c",
    "clean": "rm -rf dist"
  },
  "keywords": [
    "design-system",
    "design-tokens",
    "tokens",
    "mumukji"
  ],
  "devDependencies": {
    "@rollup/plugin-typescript": "^12.3.0",
    "rollup": "^4.59.0",
    "rollup-plugin-dts": "^6.3.0"
  }
}
