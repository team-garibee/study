{
  "name": "@mumukji/ui",
  "version": "0.0.0",
  "description": "A React-based design system for Mumukji. Includes reusable UI components built with accessibility and scalability in mind.",
  "private": false,
  "author": "team-garibee",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/team-garibee/mumukji-ui.git",
    "directory": "packages/ui"
  },
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "watch": "rollup -cw",
    "typecheck": "tsc --noEmit",
    "build": "tsc && rollup -c",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "@mumukji/icons": "workspace:*",
    "@mumukji/tokens": "workspace:*"
  },
  "peerDependencies": {
    "react": "^18.0.0 || ^19.0.0",
    "react-dom": "^18.0.0 || ^19.0.0"
  },
  "keywords": [
    "design-system",
    "react",
    "ui",
    "component-library",
    "mumukji"
  ],
  "devDependencies": {
    "@types/react": "^19.2.14",
    "@types/react-dom": "^19.2.3",
    "react": "^19.2.4",
    "rollup": "^4.58.0"
  }
}
