{
  "name": "@mumukji/ui",
  "version": "0.0.0",
  "description": "A React-based design system for Mumukji. Includes reusable UI components built with accessibility and scalability in mind.",
  "private": false,
  "author": "team-garibee",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/team-garibee/mumukji-ui.git",
    "directory": "packages/ui"
  },
  "main": "dist/index.cjs.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.cjs.js",
      "import": "./dist/index.esm.js",
      "types": "./dist/index.d.ts"
    },
    "./styles": "./dist/index.css"
  },
  "sideEffects": ["./dist/index.css"],
  "scripts": {
    "typecheck": "tsc --noEmit",
    "build": "rollup -c",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "@mumukji/icons": "workspace:*",
    "@mumukji/tokens": "workspace:*"
  },
  "peerDependencies": {
    "react": "^18.0.0 || ^19.0.0",
    "react-dom": "^18.0.0 || ^19.0.0"
  },
  "keywords": [
    "design-system",
    "react",
    "ui",
    "component-library",
    "mumukji"
  ],
  "devDependencies": {
    "@rollup/plugin-typescript": "^12.3.0",
    "@types/react": "^19.2.14",
    "@types/react-dom": "^19.2.3",
    "react": "^19.2.4",
    "rollup": "^4.59.0",
    "rollup-plugin-dts": "^6.3.0",
    "rollup-plugin-postcss": "^4.0.2",
    "sass": "^1.97.3"
  }
}
